    Min = 0,
    Max = 10,
    Callback = function(ankle)
        Kingston.Misc["Fly"].Velocity[1] = ankle
    end
}

Fly:Slider{
    Name = "Y",
    Default = Kingston.Misc["Fly"].Velocity[2],
    Min = 0,
    Max = 10,
    Callback = function(ankle)
        Kingston.Misc["Fly"].Velocity[2] = ankle
    end
}

Fly:Slider{
    Name = "Z",
    Default = Kingston.Misc["Fly"].Velocity[3],
    Min = 0,
    Max = 10,
    Callback = function(ankle)
        Kingston.Misc["Fly"].Velocity[3] = ankle
    end
}

Fly:Keybind{
    Name = "Keybind",
    Keybind = Kingston.Misc["Fly"].Keybind,
    Description = nil,
    Callback = function(key)
        Kingston.Misc["Fly"].Keybind = key
    end
}
    end
